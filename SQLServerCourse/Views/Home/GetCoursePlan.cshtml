@model List<string>

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12 text-center text-white">
            @for (int i = 0; i < Model.Count() - 1; i++)
            {
                <br />
                <p class="text-white fs-5">Урок @(i + 1). @Model[i]</p>
            }
        </div>
    </div>
</div>
